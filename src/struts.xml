<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundtion//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="com.internousdev.webshop.action" extends="struts-default">
		<action name="ShoppingAction" class="com.internousdev.webshop.action.ShoppingAction"
			method="execute">
			<result name="success">shopping.jsp</result>
			<result name="error">shoppingError.jsp</result>
		</action>

		<action name="StyleAction" class="com.internousdev.webshop.action.StyleAction"
			method="execute">
			<result name="success">style.jsp</result>
		</action>

		<action name="ReserveAction" class="com.internousdev.webshop.action.ReserveAction"
			method="execute">
			<result name="success">reserve.jsp</result>
		</action>
	</package>
</struts>